{
    "manifest_version": 3,
    "name": "Dataroom Chatbot",
    "version": "1.0",
    "description": "Chat with your Google Drive dataroom documents",
    "permissions": [
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_title": "Dataroom Chatbot"
    },
    "host_permissions": [
      "http://localhost:8000/*"
    ]
}